{
  "deno.enablePaths": [
    "./"
  ],
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false,
  "editor.inlayHints.enabled": "off",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}